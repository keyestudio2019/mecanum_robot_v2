{
    "name": "pxt-mecanum-robot-V2",
    "version": "1.0.0",
    "description": "keyestudio mecanum robot V2 for microbit",
    "dependencies": {
        "core": "*",
        "neopixel": "github:mworkfun/pxt-neopixel-bluetooth",
        "pxt-ir-remote": "github:lgm42/pxt-ir-remote#1ffda65615bf132906f65c7375cf4a93a5052f2d",
        "microphone": "*"
    },
    "files": [
        "README.md",
        "main.ts",
        "_locales/fr/pxt-mecanum-robot-V2-strings.json"
    ],
    "testFiles": [
        "test.ts"
    ],
    "public": true,
    "targetVersions": {
        "target": "5.0.6",
        "targetId": "microbit"
    },
    "supportedTargets": [
        "microbit"
    ],
    "preferredEditor": "tsprj",
    "yotta": {
        "config": {
            "microbit-dal": {
                "bluetooth": {
                    "open": 1,
                    "whitelist": 0,
                    "security_level": null
                }
            }
        }
    }
}
